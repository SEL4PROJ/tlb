                  ite(var_b"b",
                      mb(const("PC",@{typ "'b state_scheme \<Rightarrow> (32 word \<times> 'b state_scheme)"},@{theory ARM_Monadic}),
                         close
                           (var_w("v",32),
                            mb(mb(mb(mu(ex(var_w("v",32),ln 31,ln 1,(Word_Lib.mk_wordT 31)),qTy),

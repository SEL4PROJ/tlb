                       mop(Cast(@{typ string}),mop(Cast(@{typ "bool list"}),var_w("opc2",16))),
                       ls"; Thumb2; "]),
                   (call("MachineCode.BadCode",@{typ MachineCode},var_s"x",@{theory ARM_Monadic}),var_s"x")],
                  @{context}),var_s"s"],@{theory ARM_Monadic}),@{theory ARM_Monadic})))
end
\<close>


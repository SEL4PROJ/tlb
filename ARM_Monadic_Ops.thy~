                                                                                ite(var_b"b",
                                                                                    mb(call
                                                                                         ("CurrentInstrSet",
                                                                                          @{typ
                                                                                          "'b state_scheme \<Rightarrow>
                                                                                           (InstrSet \<times>
                                                                                            'b state_scheme)"},
                                                                                          lu,
                                                                                          @{theory ARM_Monadic}),
                                                                                       close
                                                                                         (var("v",
                                                                                              @{typ InstrSet}),

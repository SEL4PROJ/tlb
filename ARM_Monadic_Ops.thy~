                                                                                @{typ
                                                                                "PSR \<times> bool"}),
                                                                            mu(rupd
                                                                                 ("PSR.Q",
                                                                                  var("v",
                                                                                      @{typ
                                                                                      "PSR \<times>
                                                                                       bool"}),
                                                                                  @{theory ARM_Monadic}),qTy))),
                                                                      close

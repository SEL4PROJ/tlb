         close
           (var_w("operand2",16),
            let'
              (var_i"sh16",bop(Exp,li 2,ln 16),
               mb(call
                    ("R",@{typ "'b state_scheme \<Rightarrow> (32 word \<times> 'b state_scheme)"},var_w("n",4),
                     @{theory ARM_Monadic}),
                  close

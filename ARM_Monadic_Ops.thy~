                                                   mb(mb(mb(mu(mop(Cast w32,var_v"v"),
                                                               @{typ "bool list \<times> 'b state_scheme"}),
                                                            close
                                                              (var_w("v",32),
                                                               mu(tp[var_w("v",32),var_w("d",4)],
                                                                  @{typ "bool list \<times> 'b state_scheme"}))),

        (var_n"msbit",bop(Add,var_n"lsbit",var_n"widthminus1"),
         mb(ite(var_b"unsigned",
                mb(call
                     ("R",@{typ "'b state_scheme \<Rightarrow> (32 word \<times> 'b state_scheme)"},var_w("n",4),
                      @{theory ARM_Monadic}),

                                                                                      ("exception.UNPREDICTABLE",
                                                                                       @{typ exception},
                                                                                       ls
                                                                                        "DataProcessingPC",
                                                                                       @{theory ARM_Monadic}),
                                                                                    @{theory ARM_Monadic}),

         close
           (var_b"b",
            mu(ite(var_b"b",call("instruction.Undefined",@{typ instruction},lw(0,32),@{theory ARM_Monadic}),
                   Term.Const(@{const_name "NoOperation"},@{typ instruction})),qTy)))))
end

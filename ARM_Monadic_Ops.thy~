                              b::bool \<leftarrow> NullCheckIfThumbEE n;
                              if b then do {
                                          address::32 word \<leftarrow> R n;
                                          v::8 word \<leftarrow> MemA (Addr address, 1::nat);
                                          _::unit \<leftarrow> do {
                                                       v::32 word \<leftarrow> return (word_of_int (uint v));
                                                       return (v, t)
                                                     } \<bind> write'R;
                                          IncPC ()
                                        }
                              else return ()
                            }"



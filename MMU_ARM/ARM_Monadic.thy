                                  return (v, Addr Rn, 4) \<bind> write'MemA
                                };
                              v \<leftarrow> ROR (data, 8 * nat (uint ((word_extract 1 0 Rn)::2 word)));
                              return (v, t) \<bind> write'R
                            };
                 IncPC ()
               }"



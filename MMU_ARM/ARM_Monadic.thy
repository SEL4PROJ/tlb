definition
"Fetch"
  :: "'b::mem_op state_scheme \<Rightarrow> MachineCode \<times> 'b state_scheme"
  where
"Fetch \<equiv> do {
  iset \<leftarrow> CurrentInstrSet ();
  b \<leftarrow> if iset = InstrSet_ARM then return True else do {
                                                      v \<leftarrow> read_state Architecture;
                                                      return (v = ARMv4)
                                                    };
  if b then do {
              _ \<leftarrow> update_state (Encoding_update (\<lambda>_. Encoding_ARM));
              v \<leftarrow> read_state state.REG;
              v \<leftarrow> do {
                     v \<leftarrow> return (v RName_PC);
                     return (Addr v, 4)
                   } \<bind> MemA;
              return (ARM v)
            }
  else if iset = InstrSet_Jazelle then return (BadCode ''Fetch'')
       else do {
              v \<leftarrow> read_state state.REG;
              fpc \<leftarrow> return (v RName_PC);
              ireg \<leftarrow> MemA (Addr fpc, 2);
              if ((word_extract 15 13 ireg)::3 word) = 7 \<and> ((word_extract 12 11 ireg)::2 word) \<noteq> 0 then do {
                                                                                   _ \<leftarrow> update_state (Encoding_update (\<lambda>_. Encoding_Thumb2));
                                                                                   v \<leftarrow> MemA (Addr (fpc + 2), 2);
                                                                                   v \<leftarrow> return (ireg, v);
                                                                                   return (Thumb2 v)
                                                                                 }
              else do {
                     _ \<leftarrow> update_state (Encoding_update (\<lambda>_. Encoding_Thumb));
                     b \<leftarrow> if iset = InstrSet_Thumb then return True else do {
                                                                           v \<leftarrow> HaveThumbEE ();
                                                                           return (\<not> v)
                                                                         };
                     return (if b then Thumb ireg else ThumbEE ireg)
                   }
            }
}"


